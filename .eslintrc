{
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "jquery": true
    },
    "settings": {
      "react": {
        "pragma": "React",
        "version": "16.7.0"
      }
    },
    "rules": {
      "react/prop-types": 0,
      "react/no-find-dom-node": 0
    }
  }